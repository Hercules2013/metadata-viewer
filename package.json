{
  "name": "metadata-fixer",
  "productName": "MetaData Fixer",
  "version": "2.1.3",
  "main": "dist-electron/main/index.js",
  "description": "MetaData Fixer for Google Photos",
  "author": "Sandeep Acharya <i.am.sandeep.acharya@gmail.com>",
  "license": "MIT",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "scripts": {
    "dev": "vite",
    "compile": "tsc && vite build",
    "build": "npm run compile && electron-builder",
    "preview": "vite preview",
    "pree2e": "vite build --mode=test",
    "e2e": "playwright test",
    "pack": "npm run compile && electron-builder --dir --x64",
    "pure:pack": "electron-builder --dir --x64",
    "dist:mac": "npm run compile && electron-builder --mac --x64",
    "dist:win": "npm run compile && electron-builder --win --x64",
    "dist": "npm run compile && electron-builder --win --mac --x64",
    "release:win": "npm run compile && electron-builder --win --x64 --publish=always",
    "release:mac": "npm run compile && electron-builder --mac --x64 --publish=always"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource-variable/dm-sans": "^5.0.3",
    "@reduxjs/toolkit": "^2.2.1",
    "@stripe/react-stripe-js": "^2.3.0",
    "@stripe/stripe-js": "^2.1.5",
    "check-disk-space": "^3.4.0",
    "clsx": "^2.0.0",
    "electron-log": "^5.0.0-beta.29",
    "electron-shared-state": "^1.1.0",
    "electron-updater": "^6.1.4",
    "framer-motion": "^10.16.4",
    "fs-extra": "^11.1.1",
    "geo-tz": "^7.0.7",
    "heic-convert": "^1.2.3",
    "hidefile": "^3.0.0",
    "humanize": "^0.0.9",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "node-stream-zip": "^1.15.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^4.11.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.18.0",
    "sort-by": "^0.0.2",
    "tz-offset": "^0.0.2",
    "utimes": "^5.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@electron/notarize": "^2.1.0",
    "@playwright/test": "^1.37.1",
    "@types/heic-convert": "^1.2.2",
    "@types/hidefile": "^3.0.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/tz-offset": "^0.0.1",
    "@types/uuid": "^9.0.4",
    "@vitejs/plugin-react": "^4.1.1",
    "axios": "^1.5.1",
    "del-cli": "^5.1.0",
    "electron": "^27.0.4",
    "electron-builder": "^24.6.4",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.66.1",
    "tree-kill": "^1.2.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-electron": "^0.15.4",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  }
}
